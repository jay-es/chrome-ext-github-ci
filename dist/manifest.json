{
  "manifest_version": 3,
  "name": "chrome-ext-github-ci",
  "version": "0.3.0",
  "description": "",

  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://github.com/*"],
      "resources": ["ok.mp3", "ng.mp3"]
    }
  ]
}
